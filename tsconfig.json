{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.tsx", "src/types/*.d.ts"],
  "exclude": ["**/node_modules", "**/Pods"]
}